#define MAX 10

#include <stdio.h>

typedef struct {
    int elem[MAX];
    int count;
} List;

List initialize(List L);
List insertPos(List L, int data, int position);
List deletePos(List L, int position);
int locate(List L, int data);
List insertSorted(List L, int data);
void display(List L);

int main() {
    List L;
    initialize(L);
    
    L.elem[0] = 1;
    L.elem[1] = 2;
    L.elem[2] = 3;
    L.count = 3;
    
    // for (int i = 0; i < L.count; i++){
    //     printf("%d ", L.elem[i]);
    // }

    
    insertPos(L, 4, 1);
    insertPos(L, 5, 2);

    return 0;
}

List initialize(List L){
    L.count = 0;
}

